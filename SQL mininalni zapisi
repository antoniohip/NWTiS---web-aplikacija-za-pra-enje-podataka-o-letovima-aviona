SELECT count(distinct ident) from nwtis_ahip20_bp_2.MYAIRPORTS;

SELECT count(distinct flightDate) FROM nwtis_ahip20_bp_2.MYAIRPORTSLOG;
SELECT distinct flightDate FROM nwtis_ahip20_bp_2.MYAIRPORTSLOG

SELECT count(*)  FROM nwtis_ahip20_bp_2.AIRPLANES

SELECT count(*) from METEO
select ident, count(*) from METEO group by ident
